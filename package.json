{
  "name": "mist",
  "version": "2.0.0",
  "private": true,
  "description": "A boilerplate for using Koa2 with TypeScript",
  "main": "dist/server/index.js",
  "scripts": {
    "start": "NODE_ENV=production node dist/server/index.js",
    "test": "tsc && mocha build/test",
    "lint": "tslint src/**/*.ts",
    "build": "npm run build:client && npm run build:server",
    "build:client": "node build/build.js",
    "build:server": "tsc -p src/server/tsconfig.json",
    "dev": "tsc-watch -p src/server/tsconfig.json --onSuccess 'node ./dist/server/index.js'"
  },
  "keywords": [
    "typescript",
    "koa",
    "koa2"
  ],
  "author": "obedm503",
  "license": "MIT",
